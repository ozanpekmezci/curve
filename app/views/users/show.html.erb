<p><%=@user.name%></p>
<p><%=@user.email%></p>
<p><%=@user.country%></p>
<p><%=@user.city%></p>
<section class="stats">
       <%= render 'shared/stats' %>
</section>
<%= render 'follow_form' if user_signed_in? %>
<div class = "panel panel-default" id ="posts">
  <div class "panel-body">
    <ul class= "media-list">
      <%= render @posts, user: current_user %>
    </ul>
  </div>
</div>
